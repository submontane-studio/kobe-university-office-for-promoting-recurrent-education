{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "cipher": {
      "type": "stdio",
      "command": "cipher",
      "args": [
        "--mode",
        "mcp",
        "--agent",
        "./memAgent/cipher.yml"
      ],
      "env": {
        "OLLAMA_BASE_URL": "http://localhost:11434/v1"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "@playwright/mcp@latest",
        "--extension"
      ],
      "env": {}
    },
    "brave-search": {
      "command": "bunx",
      "args": [
        "-y",
        "brave-search-mcp"
      ],
      "env": {
        "BRAVE_API_KEY": "BSAPC_TmTWLEtl27YC3ziHPfJyu9-Xu"
      }
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--port",
        "32123"
      ]
    }
  }
}